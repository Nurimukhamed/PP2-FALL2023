11)
x1 = int(input())
y1 = int(input())
x2 = int(input())
y2 = int(input())
if (abs(x1 - x2) == 1 and abs(y1 - y2) == 2) or (abs(x1 - x2) == 2 and abs(y1 - y2) == 1):
    print("YES")
else:
    print("NO")
12)
q = int (input())
w = int (input())
e = int (input())
if(e%q==0 or e%w==0) and (e<=q*w):
    print("YES")
else:
    print("NO")
13)N=int(input())
M=int(input())
x=int(input())
y=int(input())
max=N
min=M
if(min>max): min=N
if(min==N): max=M

if(min/2>x): mind1=x
else: mind1=min-x
if(max/2>y): mind2=y
else: mind2=max-y

if(mind1<mind2): print(mind1)
else: print(mind2)
